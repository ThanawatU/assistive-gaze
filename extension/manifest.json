{
  "manifest_version": 3,
  "name": "Assistive Gaze",
  "version": "1.0",
  "description": "Eye gaze tracking with calibration",
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["math.min.js", "content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
